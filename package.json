    "debug": "~2.6.9",
    "express": "~4.15.5",
    "hbs": "~4.0.1",
    "mongoose": "^4.13.5",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "sendgrid": "^5.2.3",
    "serve-favicon": "~2.4.5",
    "stripe": "^5.4.0"
  }
}
